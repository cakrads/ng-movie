{
  "version": 2,
  "public": true,
  "name": "ng-movie",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/ng-movie/**"
    }
  },
  "build": {
    "command": "npm install --legacy-peer-deps && npm run vercel-build"
  }
}