{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/ng-movie",
  "public": true,
  "name": "ng-movie",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/ng-movie/**"
    }
  }
}